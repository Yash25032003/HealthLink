	<div class="calc-entity">
		<div class="calc-header">
			<h3 class="calc-title font18">Daily recommended fluid intake</h3>

			<div class="calc-header-controls">
				<span class="calc-header-control imperial">Imperial</span>
				<span class="calc-header-control metric">Metric</span>
			</div>
		</div>
